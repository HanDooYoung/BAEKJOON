import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		int N,sum=0;
		String value;
		
		N=sc.nextInt();
		value=sc.next();
		String array[]=value.split("");
		for(int i=0;i<N;i++) {
			sum+=Integer.parseInt(array[i]);
		}
		System.out.println(sum);
	}

}
