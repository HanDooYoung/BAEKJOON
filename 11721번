import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		String value;
		
		value=sc.next();
		String array[]=value.split("");
		for(int i=0;i<array.length;i++) {
			System.out.print(array[i]);
			if(i%10==9) {
				System.out.println();
			}
		}
	}

}
