import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		
		int point,result=0;
		for(int i=0;i<5;i++) {
			point=sc.nextInt();
			if(point<40) {
				point=40;
			}
			result+=point;
		}
		System.out.println(result/5);
	}

}
