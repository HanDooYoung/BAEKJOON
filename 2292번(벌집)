import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int N, add = 6, temp = 1, count = 1;
		N = sc.nextInt();
		while (temp < N) {
			count++;
			temp += add;
			add += 6;
		}
		System.out.println(count);
	}
}
